{
  "jupyter.lab.toolbars": {
    "FileBrowser": [
      {
        "name": "new-directory",
        "disabled": true
      },
      { "name": "uploader", "disabled": true },
      { "name": "fileNameSearcher", "disabled": true }
    ]
  },
  "title": "'@jupyter/drives-browser:file-browser-toolbar",
  "description": "jupyter-drives-browser settings.",
  "type": "object",
  "jupyter.lab.transform": true,
  "properties": {},
  "additionalProperties": false,
  "definitions": {
    "toolbarItem": {
      "properties": {
        "name": {
          "title": "Unique name",
          "type": "string"
        },
        "args": {
          "title": "Command arguments",
          "type": "object"
        },
        "command": {
          "title": "Command id",
          "type": "string",
          "default": ""
        },
        "disabled": {
          "title": "Whether the item is ignored or not",
          "type": "boolean",
          "default": false
        },
        "icon": {
          "title": "Item icon id",
          "description": "If defined, it will override the command icon",
          "type": "string"
        },
        "label": {
          "title": "Item label",
          "description": "If defined, it will override the command label",
          "type": "string"
        },
        "caption": {
          "title": "Item caption",
          "description": "If defined, it will override the command caption",
          "type": "string"
        },
        "type": {
          "title": "Item type",
          "type": "string",
          "enum": ["command", "spacer"]
        },
        "rank": {
          "title": "Item rank",
          "type": "number",
          "minimum": 0,
          "default": 50
        }
      },
      "required": ["name"],
      "additionalProperties": false,
      "type": "object"
    }
  }
}
